{
  "presets": [
    ["@babel/preset-env", {"useBuiltIns": "usage", "corejs": { "version": 3, "proposals": "true" }}],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties", [
      "styled-jsx/babel",
      {
        "plugins": [
          ["styled-jsx-plugin-sass", {
            "sassOptions": {
              "file": "./src/ui/main.scss",
              "includePaths": ["./src/ui"],
              "outFile": "./build/css/main.css"
            }
            }
          ]
        ]
      }
    ]
  ]
}
